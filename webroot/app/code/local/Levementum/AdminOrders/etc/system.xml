<config>
    <sections>
        <sales translate="label" module="sales">
            <groups>
                <adminorders translate="label">
                    <label>Admin Orders</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>50</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>0</show_in_website>
                    <show_in_store>0</show_in_store>
                    <fields>
                        <salesperson_role translate="label comment">
                            <label>Salesperson Role</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminorders/system_config_source_adminroles</source_model>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>0</show_in_store>
                        </salesperson_role>
                    </fields>
                </adminorders>
            </groups>
        </sales>
    </sections>
</config>
